			ToggleText.Font = Enum.Font.GothamSemibold
			ToggleText.Text = text
			ToggleText.TextColor3 = Color3.fromRGB(240, 240, 240)
			ToggleText.TextScaled = true
			ToggleText.TextSize = 12.000
			ToggleText.TextWrapped = true
			ToggleText.TextXAlignment = Enum.TextXAlignment.Left

			local ToggleFrameOutline = Instance.new("Frame")
			ToggleFrameOutline.Name = "ToggleFrameOutline"
			ToggleFrameOutline.Parent = Toggle
			ToggleFrameOutline.AnchorPoint = Vector2.new(0.5, 0.5)
			ToggleFrameOutline.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
			ToggleFrameOutline.BorderSizePixel = 0
			ToggleFrameOutline.Position = UDim2.new(0.959999979, 0, 0.5, 0)
			ToggleFrameOutline.Size = UDim2.new(0, 18, 0, 18)

			local ToggleFrameInline = Instance.new("Frame")
			ToggleFrameInline.Name = "ToggleFrameInline"
			ToggleFrameInline.Parent = ToggleFrameOutline
			ToggleFrameInline.AnchorPoint = Vector2.new(0.5, 0.5)
			ToggleFrameInline.BackgroundColor3 = Color3.fromRGB(45, 43, 50)
			ToggleFrameInline.BorderSizePixel = 0
			ToggleFrameInline.Position = UDim2.new(0.5, 0, 0.5, 0)
			ToggleFrameInline.Size = UDim2.new(1, -2, 1, -2)

			local ToggleFrameInlineCorner = Instance.new("UICorner")
			ToggleFrameInlineCorner.CornerRadius = UDim.new(0, 4)
			ToggleFrameInlineCorner.Name = "ToggleFrameInlineCorner"
			ToggleFrameInlineCorner.Parent = ToggleFrameInline

			local ToggleBtnImage = Instance.new("ImageButton")
			ToggleBtnImage.Name = "ToggleBtnImage"
			ToggleBtnImage.Parent = ToggleFrameInline
			ToggleBtnImage.AnchorPoint = Vector2.new(0.5, 0.5)
			ToggleBtnImage.BackgroundTransparency = 1.000
			ToggleBtnImage.Position = UDim2.new(0.5, 0, 0.5, 0)
			ToggleBtnImage.Size = UDim2.new(1, 0, 1, 0)
			ToggleBtnImage.ZIndex = 2
			ToggleBtnImage.Image = "rbxassetid://3926305904"
			ToggleBtnImage.ImageColor3 = Color3.fromRGB(160, 160, 160)
			ToggleBtnImage.ImageRectOffset = Vector2.new(284, 4)
			ToggleBtnImage.ImageRectSize = Vector2.new(24, 24)

			local ToggleFrameOutlineCorner = Instance.new("UICorner")
			ToggleFrameOutlineCorner.CornerRadius = UDim.new(0, 4)
			ToggleFrameOutlineCorner.Name = "ToggleFrameOutlineCorner"
			ToggleFrameOutlineCorner.Parent = ToggleFrameOutline

			local ToggleHolderCorner = Instance.new("UICorner")
			ToggleHolderCorner.CornerRadius = UDim.new(0, 4)
			ToggleHolderCorner.Name = "ToggleHolderCorner"
			ToggleHolderCorner.Parent = ToggleHolder
			
			local toggled = false
			Toggle.MouseButton1Click:Connect(function()
				toggled = not toggled
				if toggled then
					game.TweenService:Create(ToggleBtnImage, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						ImageColor3 = Color3.fromRGB(220, 220, 220)
					}):Play()
					game.TweenService:Create(ToggleFrameOutline, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(120, 120, 120)
					}):Play()
					game.TweenService:Create(ToggleBtnImage, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						Size = UDim2.new(1, -6, 1, -6)
					}):Play()
					wait(.2)
					ToggleBtnImage.ImageRectOffset = Vector2.new(312, 4)
					game.TweenService:Create(ToggleBtnImage, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						Size = UDim2.new(1, 0, 1, 0)
					}):Play()
				else
					toggled = false
					game.TweenService:Create(ToggleBtnImage, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						ImageColor3 = Color3.fromRGB(160, 160, 160)
					}):Play()
					game.TweenService:Create(ToggleFrameOutline, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(60, 60, 60)
					}):Play()
					game.TweenService:Create(ToggleBtnImage, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						Size = UDim2.new(1, -6, 1, -6)
					}):Play()
					wait(.2)
					ToggleBtnImage.ImageRectOffset = Vector2.new(284, 4)
					game.TweenService:Create(ToggleBtnImage, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						Size = UDim2.new(1, 0, 1, 0)
					}):Play()
				end
				pcall(callback, toggled)
			end)
		end
		
		function pages:Slider(slidertext, min, max, start, inc, callback)
			local dragging = false
			
			local Slider = Instance.new("Frame")
			Slider.Name = "Slider"
			Slider.Parent = Page
			Slider.AnchorPoint = Vector2.new(0.5, 0.5)
			Slider.BackgroundColor3 = Color3.fromRGB(45, 43, 50)
			Slider.BackgroundTransparency = 0
			Slider.BorderSizePixel = 0
			Slider.Position = UDim2.new(0.5, 0, 0.835978866, 0)
			Slider.Size = UDim2.new(0, 450, 0, 40)

			local SliderCorner = Instance.new("UICorner")
			SliderCorner.CornerRadius = UDim.new(0, 4)
			SliderCorner.Name = "SliderCorner"
			SliderCorner.Parent = Slider

			local SliderText = Instance.new("TextLabel")
			SliderText.Name = "SliderText"
			SliderText.Parent = Slider
			SliderText.AnchorPoint = Vector2.new(0, 0)
			SliderText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			SliderText.BackgroundTransparency = 1.000
			SliderText.BorderSizePixel = 0
			SliderText.Position = UDim2.new(0, 10,0, 3)
			SliderText.Size = UDim2.new(0, 333, 0, 20)
			SliderText.Font = Enum.Font.GothamSemibold
			SliderText.Text = slidertext
			SliderText.TextColor3 = Color3.fromRGB(238, 238, 238)
			SliderText.TextSize = 12.000
			SliderText.TextWrapped = true
			SliderText.TextXAlignment = Enum.TextXAlignment.Left

			local SliderValue = Instance.new("TextLabel")
			SliderValue.Name = "SliderValue"
			SliderValue.Parent = Slider
			SliderValue.AnchorPoint = Vector2.new(0, 0)
			SliderValue.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			SliderValue.BackgroundTransparency = 1.000
			SliderValue.BorderSizePixel = 0
			SliderValue.Position = UDim2.new(0, 340,0, 3)
			SliderValue.Size = UDim2.new(0, 99, 0, 20)
			SliderValue.Font = Enum.Font.GothamSemibold
			SliderValue.Text = ""
			SliderValue.TextColor3 = Color3.fromRGB(238, 238, 238)
			SliderValue.TextSize = 12.000
			SliderValue.TextWrapped = true
			SliderValue.TextXAlignment = Enum.TextXAlignment.Right

			local SlideHolder = Instance.new("Frame")
			SlideHolder.Name = "SlideHolder"
			SlideHolder.Parent = Slider
			SlideHolder.BackgroundColor3 = Color3.fromRGB(39, 38, 44)
			SlideHolder.BorderSizePixel = 0
			SlideHolder.Position = UDim2.new(0, 8,0, 26)
			SlideHolder.Size = UDim2.new(0, 434, 0, 6)
			SlideHolder.AnchorPoint = Vector2.new(0, 0)

			local SlideHolderCorner = Instance.new("UICorner")
			SlideHolderCorner.CornerRadius = UDim.new(0, 2)
			SlideHolderCorner.Name = "SlideHolderCorner"
			SlideHolderCorner.Parent = SlideHolder

			local Slide = Instance.new("Frame")
			Slide.Name = "Slide"
			Slide.Parent = SlideHolder
			Slide.BackgroundColor3 = Color3.fromRGB(55, 53, 62)
			Slide.BorderSizePixel = 0
			Slide.Size = UDim2.new(0, 217, 1, 0)

			local SlideCorner = Instance.new("UICorner")
			SlideCorner.CornerRadius = UDim.new(0, 2)
			SlideCorner.Name = "SlideHolderCorner"
			SlideCorner.Parent = Slide
			
			local function move(Input) --math not be me btw
				local XSize = math.clamp((Input.Position.X - SlideHolder.AbsolutePosition.X) / SlideHolder.AbsoluteSize.X, 0, 1)
				local Increment = inc and (max / ((max - min) / (inc * 4))) or 
					(max >= 50 and max / ((max - min) / 4)) or 
					(max >= 25 and max / ((max - min) / 2)) or 
					(max / (max - min))

				local SizeRounded = UDim2.new((math.round(XSize * ((max / Increment) * 4)) / ((max / Increment) * 4)), 0, 1, 0)
				Slide:TweenSize(SizeRounded, Enum.EasingDirection.Out, Enum.EasingStyle.Quad, .1, true)

				local Val = math.round((((SizeRounded.X.Scale * max) / max) * (max - min) + min) * 20) / 20
				SliderValue.Text = tostring(Val)
				pcall(callback, tonumber(Val))
			end

			SlideHolder.InputBegan:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseButton1 then
					dragging = true
				end
			end)

			SlideHolder.InputEnded:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseButton1 then
					dragging = false
				end
			end)

			game:GetService("UserInputService").InputChanged:Connect(function(input)
				if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
					move(input)
				end
			end)
		end
		
		function pages:Dropdown(name, default, options, cb)
			local opened = false
			assert(type(options) == "table", "options must be a table")
			assert(type(cb) == "function", "callback must be a function")
			local f = tostring(name) .. " - %s"

			local Dropdown = Instance.new("Frame")
			Dropdown.Name = "Dropdown"
			Dropdown.Parent = Page
			Dropdown.AnchorPoint = Vector2.new(0.5, 0.5)
			Dropdown.BackgroundColor3 = Color3.fromRGB(45, 43, 50)
			Dropdown.BorderSizePixel = 0
			Dropdown.Position = UDim2.new(0.5, 0, 0.5, 0)
			Dropdown.Size = UDim2.new(0, 450, 0, 28)
			Dropdown.ZIndex = 99

			local DropdownText = Instance.new("TextLabel")
			DropdownText.Name = "DropdownText"
			DropdownText.Parent = Dropdown
			DropdownText.AnchorPoint = Vector2.new(0.5, 0.5)
			DropdownText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			DropdownText.BackgroundTransparency = 1.000
			DropdownText.BorderSizePixel = 0
			DropdownText.Position = UDim2.new(0.419999987, 0, 0.5, 0)
			DropdownText.Size = UDim2.new(0, 361, 0, 12)
			DropdownText.Font = Enum.Font.GothamSemibold
			DropdownText.Text = string.format(f, default)
			DropdownText.TextColor3 = Color3.fromRGB(240, 240, 240)
			DropdownText.TextScaled = true
			DropdownText.TextSize = 12.000
			DropdownText.TextWrapped = true
			DropdownText.TextXAlignment = Enum.TextXAlignment.Left

			local DropdownCorner = Instance.new("UICorner")
			DropdownCorner.CornerRadius = UDim.new(0, 4)
			DropdownCorner.Name = "DropdownCorner"
			DropdownCorner.Parent = Dropdown

			local DropdownArrow = Instance.new("ImageLabel", Dropdown)
			DropdownArrow.AnchorPoint = Vector2.new(0, 0)
			DropdownArrow.BackgroundTransparency = 1
			DropdownArrow.Position = UDim2.new(.95, 0, 0, 5)
			DropdownArrow.Size = UDim2.new(0, 22, 1, -6)
			DropdownArrow.Image = "rbxassetid://3926307971"
			DropdownArrow.ImageColor3 = Color3.fromRGB(107, 107, 107)
			DropdownArrow.ScaleType = Enum.ScaleType.Fit
			DropdownArrow.SliceScale = 1
			DropdownArrow.ImageRectOffset = Vector2.new(324, 524)
			DropdownArrow.ImageRectSize = Vector2.new(36, 36)

			local DropdownChildFrame = Instance.new("ScrollingFrame")
			DropdownChildFrame.Name = "DropdownChildFrame"
			DropdownChildFrame.Parent = Dropdown
			DropdownChildFrame.Active = true
			DropdownChildFrame.BackgroundColor3 = Color3.fromRGB(45, 43, 50)
			DropdownChildFrame.BorderSizePixel = 0
			DropdownChildFrame.Position = UDim2.new(0, 0, 0, 26)
			DropdownChildFrame.Size = UDim2.new(1, 0, 0, 0)
			DropdownChildFrame.Visible = false
			DropdownChildFrame.ScrollBarThickness = 0

			local DropdownChildFrameListing = Instance.new("UIListLayout")
			DropdownChildFrameListing.Name = "DropdownChildFrameListing"
			DropdownChildFrameListing.Parent = DropdownChildFrame
			DropdownChildFrameListing.HorizontalAlignment = Enum.HorizontalAlignment.Center
			DropdownChildFrameListing.SortOrder = Enum.SortOrder.LayoutOrder
			DropdownChildFrameListing.Padding = UDim.new(0, 2)

			DropdownChildFrameListing:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
				DropdownChildFrame.CanvasSize = UDim2.new(0, 0, 0, DropdownChildFrameListing.AbsoluteContentSize.Y) 
			end)

			local debounce = false

			local function toggle()
				if (debounce) then return end

				opened = not opened

				if (not opened) then
					debounce = true
				end

				local tween = TweenService:Create(
					DropdownArrow, TweenInfo.new(.2), {
						Rotation = opened and 90 or 0
					}
				)

				TweenService:Create(
					DropdownChildFrame,
					TweenInfo.new(.2), {
						Size = UDim2.new(1, 0, 0, opened and 80 or 0)
					}
				):Play()

				tween:Play()
				if (not opened) then
					tween.Completed:Wait()
					debounce = false
				end

				DropdownChildFrame.Visible = opened
			end

			Dropdown.InputBegan:Connect(function(inp)
				if (inp.UserInputType == Enum.UserInputType.MouseButton1) then
					toggle()
				end
			end)

			local pressed = false
			for _, opt in next, options do
				local ButtonCorner = Instance.new("UICorner")
				local ButtonText = Instance.new("TextLabel")
				
				local DropdownChildFrameOption = Instance.new("TextButton")
				DropdownChildFrameOption.Name = "DropdownChildFrameOption"
				DropdownChildFrameOption.Parent = DropdownChildFrame
				DropdownChildFrameOption.BackgroundColor3 = Color3.fromRGB(45, 43, 50)
				DropdownChildFrameOption.Position = UDim2.new(0.0188888889, 0, 0, 0)
				DropdownChildFrameOption.Size = UDim2.new(0, 433, 0, 20)
				DropdownChildFrameOption.AutoButtonColor = false
				DropdownChildFrameOption.Font = Enum.Font.GothamSemibold
				DropdownChildFrameOption.TextColor3 = Color3.fromRGB(120, 120, 120)
				DropdownChildFrameOption.TextSize = 12.000
				DropdownChildFrameOption.Text = tostring(opt)
				DropdownChildFrameOption.MouseButton1Click:Connect(function()
					if (pressed) then return end
					pressed = true
					DropdownText.Text = string.format(f, opt)
					coroutine.wrap(cb)(opt)
					toggle()
					pressed = false
				end)
				DropdownChildFrameOption.MouseEnter:Connect(function()
					TweenService:Create(DropdownChildFrameOption, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						TextColor3 = Color3.fromRGB(240, 240, 240)
					}):Play()
					TweenService:Create(DropdownChildFrameOption, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(56, 53, 62)
					}):Play()
				end)
				DropdownChildFrameOption.MouseLeave:Connect(function()
					TweenService:Create(DropdownChildFrameOption, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						TextColor3 = Color3.fromRGB(120, 120, 120)
					}):Play()
					TweenService:Create(DropdownChildFrameOption, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
						BackgroundColor3 = Color3.fromRGB(45, 43, 50)
					}):Play()
				end)

				local DropdownChildFrameOptionCorner = Instance.new("UICorner")
				DropdownChildFrameOptionCorner.CornerRadius = UDim.new(0, 4)
				DropdownChildFrameOptionCorner.Name = "DropdownCorner"
				DropdownChildFrameOptionCorner.Parent = DropdownChildFrameOption
			end
		end
		
		function pages:Paragraph(title, content)
			local Paragraph = Instance.new("Frame")
			Paragraph.Name = "Paragraph"
			Paragraph.Parent = Page
			Paragraph.AnchorPoint = Vector2.new(0.5, 0.5)
			Paragraph.BackgroundColor3 = Color3.fromRGB(45, 43, 50)
