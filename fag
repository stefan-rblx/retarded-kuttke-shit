		function pages:Toggle(text, callback)
			local ToggleHolder = Instance.new("Frame")
			ToggleHolder.Name = "ToggleHolder"
			ToggleHolder.Parent = Page
			ToggleHolder.AnchorPoint = Vector2.new(0.5, 0.5)
			ToggleHolder.BackgroundColor3 = Color3.fromRGB(45, 43, 50)
			ToggleHolder.BackgroundTransparency = 1.000
			ToggleHolder.BorderSizePixel = 0
			ToggleHolder.Position = UDim2.new(0.511156619, 0, 0.354497343, 0)
			ToggleHolder.Size = UDim2.new(0, 450, 0, 28)

			local Toggle = Instance.new("TextButton")
			Toggle.Name = "Toggle"
			Toggle.Parent = ToggleHolder
			Toggle.AnchorPoint = Vector2.new(0.5, 0.5)
			Toggle.BackgroundColor3 = Color3.fromRGB(45, 43, 50)
			Toggle.BorderSizePixel = 0
			Toggle.Position = UDim2.new(0.5, 0, 0.5, 0)
			Toggle.Size = UDim2.new(0, 450, 0, 28)
			Toggle.AutoButtonColor = false
			Toggle.Font = Enum.Font.SourceSans
			Toggle.Text = ""
			Toggle.TextColor3 = Color3.fromRGB(0, 0, 0)
			Toggle.TextSize = 14.000

